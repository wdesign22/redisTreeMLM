{% extends 'base.html.twig' %}

{% block body %}
    <h1>Mlmuser</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ mlmUser.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ mlmUser.name }}</td>
            </tr>
            <tr>
                <th>Ancestor</th>
                <td>{{ mlmUser.ancestor }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('redis_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('redis_edit', { 'id': mlmUser.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
